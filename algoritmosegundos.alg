algoritmo "semnome"
// Função :
// Autor :
// Data : 25/03/2017
// Seção de Declarações 
var
seg, min, hrs : real
inicio
escreva ("Número de segundos: ")
leia (seg)
min <- seg / 60
escreval ("Número de minutos: ", min, " minutos")
hrs <- seg / 3600
escreva ("Número de horas: ", hrs, " horas")
// Seção de Comandos 
fimalgoritmo
